<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.User" table="user" schema="shiguang">
        <id name="userid">
            <column name="userid" sql-type="bigint(5)"/>
        </id>
        <property name="phone">
            <column name="phone" sql-type="varchar(13)" length="13" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" />
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45" />
        </property>
        <property name="icon">
            <column name="icon" sql-type="varchar(80)" length="80"/>
        </property>
        <property name="power">
            <column name="power" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="wxid">
            <column name="wxid" sql-type="varchar(45)" length="45" />
        </property>
        <property name="qqid">
            <column name="qqid" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="sinaid">
            <column name="sinaid" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="wxname">
            <column name="wxname" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="qqname">
            <column name="qqname" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="sinaname">
            <column name="sinaname" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="tomatoN">
            <column name="tomato_n" sql-type="int(11)"/>
        </property>
        <property name="powerToday">
            <column name="power_today" sql-type="int(11)"/>
        </property>
        <property name="canSteal">
            <column name="canSteal" sql-type="int(1)" />
        </property>
    </class>
</hibernate-mapping>